{
  "buildCommand": "chmod +x *.sh && npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/cv/cv-final-video",
      "status": 302,
      "headers": {
        "Location": "https://github.com/Dingnuooo/dingnuooo-slidev/raw/refs/heads/main/public/video.mkv?download="
      }
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|woff|woff2|ico|json|txt|mp4|mkv|webm|pdf|wasm|ttf|otf))",
      "dest": "/$1"
    },
    {
      "src": "^/([a-zA-Z_-]+)/([^/]+)/(.+)$",
      "dest": "/$1/$2/$3"
    },
    {
      "src": "^/([a-zA-Z_-]+)/([^/]+)/?$",
      "dest": "/$1/$2/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
